{{#week-plan week=week as |weekPlanner|}}
  {{#link-to 'plan.week' weekPlanner.prevWeek}}Previous{{/link-to}}
  {{#link-to 'plan.week' weekPlanner.nextWeek}}Next{{/link-to}}

  <h1>Week {{week.weekNumber}}</h1>

  {{#each weekPlanner.weekDays as |day|}}
    {{#week-day day=day}}
    {{/week-day}}
  {{/each}}

  {{outlet}}
{{/week-plan}}
